{
  "request": {
    "method": "POST",
    "url": "/v1/getdevice",
    "formParameters": {
      "brand": {
        "equalTo": "Apple"
      },
      "device": {
        "equalTo": "Apple iPhone X"
      }
    }
  },

  "response": {
    "status": 200,
    "jsonBody": [{
      "DeviceName": "Apple iPhone X",
      "Brand":  "Apple",
      "technology": "GSM / HSPA / LTE",
      "_2g_bands": "GSM 850 / 900 / 1800 / 1900",
      "_3g_bands": "HSDPA 850 / 900 / 1700(AWS) / 1900 / 2100",
      "_4g_bands": "1, 2, 3, 4, 5, 7, 8, 12, 13, 17, 18, 19, 20, 25, 26, 28, 29, 30, 34, 38, 39, 40, 41, 66"
    }],
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
