{
  "request": {
    "method": "POST",
    "url": "/v1/getdevice",
    "formParameters": {
      "brand": {
        "equalTo": "Apple"
      },
      "device": {
        "equalTo": "Apple iPhone 12"
      }
    }
  },

  "response": {
    "status": 200,
    "jsonBody": [{
      "DeviceName": "Apple iPhone 12",
      "Brand":  "Apple",
      "technology": "GSM / CDMA / HSPA / EVDO / LTE / 5G",
      "_2g_bands": "GSM 850 / 900 / 1800 / 1900 - SIM 1 & SIM 2 (dual-SIM) - for China",
      "_3g_bands": "HSDPA 850 / 900 / 1700(AWS) / 1900 / 2100",
      "_4g_bands": "1, 2, 3, 4, 5, 7, 8, 12, 13, 14, 17, 18, 19, 20, 25, 26, 28, 29, 30, 32, 34, 38, 39, 40, 41, 42, 46, 48, 66, 71 - A2172"
    }],
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
