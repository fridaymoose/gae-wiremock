{
  "request": {
    "method": "POST",
    "url": "/v1/getdevice",
    "formParameters": {
      "brand": {
        "equalTo": "Motorola"
      },
      "device": {
        "equalTo": "Motorola Nexus 6"
      }
    }
  },

  "response": {
    "status": 200,
    "jsonBody": [{
      "DeviceName": "Motorola Nexus 6",
      "Brand":  "Motorola",
      "technology": "GSM / CDMA / HSPA / LTE",
      "_2g_bands": "GSM 850 / 900 / 1800 / 1900 - all models",
      "_3g_bands": "HSDPA 800 / 850 / 900 / 1700 / 1800 / 1900 / 2100 - XT1100",
      "_4g_bands": "1, 3, 5, 7, 8, 9, 19, 20, 28, 41 - XT1100"
    }],
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
