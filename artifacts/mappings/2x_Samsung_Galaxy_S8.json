{
  "request": {
    "method": "POST",
    "url": "/v1/getdevice",
    "formParameters": {
      "brand": {
        "equalTo": "Samsung"
      },
      "device": {
        "equalTo": "2x Samsung Galaxy S8"
      }
    }
  },

  "response": {
    "status": 200,
    "jsonBody": [{
      "DeviceName": "2x Samsung Galaxy S8",
      "Brand":  "Samsung",
      "technology": "GSM / HSPA / LTE",
      "_2g_bands": "GSM 850 / 900 / 1800 / 1900 - SIM 1 & SIM 2 (dual-SIM model only)",
      "_3g_bands": "HSDPA 850 / 900 / 1700(AWS) / 1900 / 2100",
      "_4g_bands": "1, 2, 3, 4, 5, 7, 8, 12, 13, 17, 18, 19, 20, 25, 26, 28, 32, 66, 38, 39, 40, 41"
    }],
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
